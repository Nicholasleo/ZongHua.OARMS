<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ZongHua.Models" namespace="ZongHua.Models">
  <class name="UserInfo" table="T_Sys_User" lazy="true">
    <id name="FID" column="FID" type="Guid">
      <generator class="assigned"></generator>
    </id>
    <property name="UserCode" type="string">
      <column name="UserCode" length="50"></column>
    </property>
    <property name="UserName" type="string">
      <column name="UserName" length="50"></column>
    </property>
    <property name="Password" type="string">
      <column name="Password" length="50"></column>
    </property>
  </class>
</hibernate-mapping>